{
	"maxerr": false,
	"text-ignore-regex": false,
	"raw-ignore-regex": false,
	"attr-name-ignore-regex": false,
	"id-class-ignore-regex": false,
	"line-max-len-ignore-regex": false,
	"rules": {
		"attr-name-style": ["error", "dash"],
		"attr-no-dup": true,
		"attr-order": ["error", ["alt", "aria-labelledby", "autocapitalize", "autocomplete", "charset", "class", "colspan", "for", "height", "href", "id", "lang", "loading", "maxlength", "name", "placeholder", "readonly", "rows", "size", "src", "style", "target", "title", "type", "value", "width"]],
		"attr-quote-style": ["error", "double"],
		"attr-req-value": true,
		"attr-validate": true,
		"button-req-content": true,
		"class-no-dup": true,
		"class-style": ["error", "bem"],
		"doctype-first": ["error", "smart"],
		"doctype-html5": true,
		"fieldset-contains-legend": true,
		"fig-req-figcaption": true,
		"focusable-tabindex-style": true,
		"head-req-title": true,
		"head-valid-content-model": true,
		"html-req-lang": true,
		"html-valid-content-model": true,
		"id-style": ["error", "bem"],
		"id-no-dup": true,
		"img-req-alt": true,
		"img-req-src": true,
		"indent-style": ["error", "tabs"],
		"indent-width": ["error", 1],
		"input-btn-req-value-or-title": true,
		"input-radio-req-name": true,
		"input-req-label": true,
		"label-no-enc-textarea-or-select": true,
		"label-req-for": true,
		"line-end-style": ["error", "lf"],
		"line-max-len": ["error", 150],
		"line-no-trailing-whitespace": true,
		"link-label-min-length": ["error", 4],
		"link-req-noopener": true,
		"table-req-header": true,
		"tag-close": true,
		"tag-name-lowercase": true,
		"tag-name-match": true,
		"tag-self-close": ["error", "never"],
		"title-no-dup": true
	}
}
